{
  "name": "font-selector",
  "version": "1.0.6",
  "description": "A Font Selector for Google/FontAwesome or an array of fonts!",
  "main": "index.js",
  "babel": {
    "presets": [
      "es2015",
      "stage-3"
    ]
  },
  "scripts": {
    "lint": "eslint src",
    "start": "cross-env WEBPACK_ENV=dev webpack-dev-server --host localhost --port 8080 --hot --progress --inline",
    "build": "npm run lint && webpack && npm version patch",
    "dev": "node dev.js",
    "test": "run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Owchzzz/googlefonts-selector.git"
  },
  "keywords": [
    "google",
    "fonts"
  ],
  "author": "Richard Abear",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Owchzzz/googlefonts-selector/issues"
  },
  "homepage": "https://github.com/Owchzzz/googlefonts-selector#readme",
  "dependencies": {
    "backbone": "^1.3.3",
    "jquery": "^3.2.1",
    "underscore": "^1.8.3",
    "webpack-dev-server": "^2.10.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^0.28.8",
    "eslint": "^4.15.0",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.7.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "webpack": "^3.10.0"
  }
}
